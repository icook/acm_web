<FilesMatch "\.(htaccess|py|db)$">
Order Allow,Deny
Deny from all
</FilesMatch>
AddHandler cgi-script .cgi
RewriteEngine On
RewriteRule ^cgi-bin - [L]
RewriteRule ^web - [L]
RewriteRule ^static/(.*)$ /~acm/web/static/$1 [L]
RewriteRule ^dynamic/(.*)$ /~acm/web/dynamic/$1 [L]
RewriteRule ^(.*)$ /~acm/cgi-bin/django.cgi/$1 [QSA,L]
